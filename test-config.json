  "production": {
    "name": "Production Environment",
    "description": "Configuration for testing in production environment",
    "database": {
      "url": "${NEXT_PUBLIC_SUPABASE_URL}",
      "serviceKey": "${SUPABASE_SERVICE_ROLE_KEY}",
      "anonKey": "${NEXT_PUBLIC_SUPABASE_ANON_KEY}"
    },
    "testUsers": [
      {
        "email": "test-user-prod@example.com",
        "password": "test-password-123",
        "role": "regular",
        "description": "Regular test user for production"
      },
      {
        "email": "test-admin-prod@example.com",
        "password": "test-admin-123",
        "role": "admin",
        "description": "Admin test user for production"
      }
    ],
    "testData": {
      "strategies": [
        {
          "name": "Production Test Strategy 1",
          "description": "First test strategy for production environment"
        },
        {
          "name": "Production Test Strategy 2",
          "description": "Second test strategy for production environment"
        }
      ],
      "trades": [
        {
          "symbol": "PROD1",
          "direction": "long",
          "entry_price": 100.00,
          "quantity": 10
        },
        {
          "symbol": "PROD2",
          "direction": "short",
          "entry_price": 200.00,
          "quantity": 5
        }
      ]
    },
    "testSettings": {
      "allowDestructiveTests": false,
      "allowDataCreation": false,
      "allowDataDeletion": false,
      "maxTestUsers": 3,
      "maxTestStrategies": 5,
      "maxTestTrades": 10,
      "performanceThresholds": {
        "queryTimeMs": 3000,
        "creationTimeMs": 50,
        "batchOperationTimeMs": 5000
      }
    },
    "notifications": {
      "sendEmailReports": true,
      "logToFile": true,
      "logLevel": "info"
    }
  },
  "common": {
    "testExecution": {
      "timeoutMs": 300000,
      "retryAttempts": 3,
      "retryDelayMs": 1000,
      "parallelTests": false,
      "cleanupAfterTest": true
    },
    "reporting": {
      "outputFormat": "json",
      "includeStackTrace": true,
      "includePerformanceMetrics": true,
      "saveDetailedResults": true,
      "reportRetentionDays": 30
    },
    "validation": {
      "checkForeignKeyConstraints": true,
      "checkRLSPolicies": true,
      "checkTableStructure": true,
      "checkDataIntegrity": true,
      "checkPerformance": true
    },
    "rollback": {
      "enabled": true,
      "createBackupBeforeTest": true,
      "backupRetentionDays": 7,
      "autoRollbackOnFailure": false,
      "manualRollbackRequired": true
    }
  }
}